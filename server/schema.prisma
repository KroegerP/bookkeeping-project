// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(uuid()) @postgresql.Uuid
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Purchase {
  id          String    @id @default(uuid()) @postgresql.Uuid
  date        DateTime  @default("2023-12-30T18:05:29.700Z")
  description String    @default("")
  cost        Float
  category    Category? @relation("Purchase_category", fields: [categoryId], references: [id])
  categoryId  String?   @map("category") @postgresql.Uuid
  createdAt   DateTime? @default(now())

  @@index([categoryId])
}

model Category {
  id        String     @id @default(uuid()) @postgresql.Uuid
  name      String     @unique @default("")
  purchases Purchase[] @relation("Purchase_category")
}
